{
  "name": "backend-core",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --minify && pnpm copyapi",
    "build:dev": "pnpm lint && pnpm clean && tsup src/index.ts --dts --format esm && pnpm copyapi",
    "clean": "rm -rf dist",
    "check": "npx @biomejs/biome check --write ./src",
    "copyapi": "cp ../../api/spec/api.yaml ./dist/ && cp -r ../../api/docs ./dist/",
    "db:sync": "pnpm db:generate && pnpm db:migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "tsx src/drizzle/migrate.ts"
  },
  "keywords": [],
  "author": "MVP Chops <mvpchops@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@t3-oss/env-core": "^0.10.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "express": "^4.19.2",
    "express-openapi-validator": "^5.1.6",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "postgres": "^3.4.4",
    "ulidx": "^2.3.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.5",
    "@types/ioredis": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "drizzle-kit": "^0.22.7",
    "tsx": "^4.15.7"
  }
}
